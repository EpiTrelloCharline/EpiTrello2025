#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules/esprima/bin/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules/esprima/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/node_modules:/home/meuh/Documents/delivery/S9/node_modules:/home/meuh/Documents/delivery/node_modules:/home/meuh/Documents/node_modules:/home/meuh/node_modules:/home/node_modules:/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules/esprima/bin/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules/esprima/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/api/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/apps/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/node_modules:/home/meuh/Documents/delivery/S9/node_modules:/home/meuh/Documents/delivery/node_modules:/home/meuh/Documents/node_modules:/home/meuh/node_modules:/home/node_modules:/node_modules:/home/meuh/Documents/delivery/S9/EpiTrello2025/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../esprima/bin/esvalidate.js" "$@"
else
  exec node  "$basedir/../esprima/bin/esvalidate.js" "$@"
fi
